<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
      res.redirect('/')
    } %>

<%-messages()%>
<form action="/">
    <label for="email">Email:</label>
    <input type="email" id="email" name="account_email" required>
    <label for="pwd">Password:</label>
    <input type="password" id="pwd" name="account_password" required>
    <input type="submit" value="Submit">
  </form>
<p>No account? <a href="/account/registration">Sign Up</a></p>
